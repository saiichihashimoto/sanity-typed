{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@sanity-typed/tsconfig/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "baseUrl": ".",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "../../@types/**/*.d.ts"
  ],
  "exclude": ["dist", "node_modules"]
}
